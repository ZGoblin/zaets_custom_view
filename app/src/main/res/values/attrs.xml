<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="FanControlView">
        <attr name="color" format="color"/>
        <attr name="fontSize" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="CoolerView">
        <attr name="src" format="reference"/>
